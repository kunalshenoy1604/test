{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites":[
        {
            "source": "/",
            "destination": "/index.js"
        },
        {
            "source": "/contact",
            "destination": "/index.js"
        },
        {
            "source": "/warranty",
            "destination": "/index.js"
        }
    ]
}